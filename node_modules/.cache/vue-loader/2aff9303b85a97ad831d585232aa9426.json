ottom\">\n    <slot></slot>\n  </transition>\n</template>\n\n<script>\nimport gsap from \"gsap\";\nimport { ScrollTrigger } from \"gsap/ScrollTrigger\";\ngsap.registerPlugin(ScrollTrigger);\n\nexport default {\n  props: {\n    delay: {\n      type: Number,\n      default: 0,\n    },\n    duration: {\n      type: Number,\n      default: 1,\n    },\n  },\n  setup(props) {\n    const fromBottom = (el) => {\n      gsap.from(el, {\n        yPercent: -30,\n        autoAlpha: 0,\n        delay: props.delay,\n        duration: props.duration,\n        scrollTrigger: {\n          trigger: el,\n        },\n      });\n    };\n    return { fromBottom };\n  },\n};\n</script>\n\n<style scoped></style>\n"]}]}